$TTL    604800
@       IN      SOA     okd4-services.okd.local. admin.okd.local. (
                  2     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800     ; Negative Cache TTL
)

; name servers - NS records
    IN      NS      okd4-services.okd.local.

; name servers - A records
okd4-services.okd.local.          IN      A       192.168.100.210

; OpenShift Container Platform Cluster - A records
okd4-bootstrap.okd.local.        IN      A       192.168.100.200
okd4-control-plane-1.okd.local.  IN      A       192.168.100.201
okd4-control-plane-2.okd.local.  IN      A       192.168.100.202
okd4-control-plane-3.okd.local.  IN      A       192.168.100.203
okd4-compute-1.okd.local.        IN      A       192.168.100.204
okd4-compute-2.okd.local.        IN      A       192.168.100.205

; OpenShift internal cluster IPs - A records
api.okd.local.        IN    A    192.168.100.210
api-int.okd.local.    IN    A    192.168.100.210
*.apps.okd.local.     IN    A    192.168.100.210
etcd-0.okd.local.     IN    A    192.168.100.201
etcd-1.okd.local.     IN    A    192.168.100.202
etcd-2.okd.local.     IN    A    192.168.100.203

; OpenShift internal cluster IPs - SRV records
_etcd-server-ssl._tcp.okd.local.   IN    SRV     0    10    2380    etcd-0.okd.local.
_etcd-server-ssl._tcp.okd.local.   IN    SRV     0    10    2380    etcd-1.okd.local.
_etcd-server-ssl._tcp.okd.local.   IN    SRV     0    10    2380    etcd-2.okd.local.
